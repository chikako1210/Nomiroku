<h3>ユーザー一覧</h3>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>ユーザー名</th>
      <th>性別</th>
      <th>生年月日</th>
      <th>登録日</th>
      <th>最終更新日</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= link_to user.name, admins_user_path(user.id) %></td>
      <td><%= user.sex %></td>
      <td><%= user.birthday %></td>
      <td><%= user.created_at.strftime('%Y/%m/%d %H:%M')%></td>
      <td><%= user.updated_at.strftime('%Y/%m/%d %H:%M')%></td>
    </tr>
    <% end %>
  </tbody>
</table>